<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.mapper.product.productContents">

    <insert id="insertProductContents" parameterType="com.shop.core.entity.Contents">
        /* com.shop.mapper.product.productContents.insertProductContents */
        INSERT INTO TB_CONTENTS (
            NEWS_TYPE ,
            NEWS_TITLE ,
        <if test='newsSubTitle != null'>
            NEWS_SUB_TITLE,
        </if>
        <if test='newsContents != null'>
            NEWS_CONTENTS,
        </if>
            CRE_USER ,
            UPD_USER
        ) VALUES (
            #{newsType} ,
            #{newsTitle} ,
        <if test='newsSubTitle != null'>
            #{newsSubTitle} ,
        </if>
        <if test='newsContents != null'>
            #{newsContents} ,
        </if>
            #{creUser} ,
            #{updUser}
        )
    </insert>
</mapper>